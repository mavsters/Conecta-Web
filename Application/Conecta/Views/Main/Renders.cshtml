@{
    ViewData["Title"] = "Blog";
    ViewData["Background"] = "bocetos/renders/main.png";
    ViewData["Title-Main"] = "Renders - último proceso del proyecto";
    ViewData["Title-Description"] = "Matta Sur Conecta con la Memoria";
    Layout = "~/Views/Shared/_LayoutBlog.cshtml";
}
<div class="col-md-12 ml-auto mr-auto">
    <div class="header text-center">
        <h3 class="title">
            Renders principales
        </h3>
    </div>
    <!--Content-->
    <div id="collapseRenders" class="collapse show" role="tabpanel" aria-labelledby="headingOne">
        <div class="card-body">
            
            
            <!-- Process -->
            <div id="CE_Proccess_0" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    @for (int i = 0; i < 6; i++)
                    {
                        <li data-target="#CE_Proccess_0" data-slide-to="@i.ToString()" class="@((i==1)?"active":"")"></li>
                    }
                </ol>
                <div class="carousel-inner">
                    @for (int i = 1; i <= 6; i++)
                    {
                        <div class="carousel-item @((i==1)?"active":"")">
                            <img class="d-block w-100" src='../../../images/bocetos/renders/@(i.ToString()+".bmp")'>
                        </div>
                    }
                </div>
                <a class="carousel-control-prev" href="#CE_Proccess_0" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Anterior</span>
                </a>
                <a class="carousel-control-Siguiente" href="#CE_Proccess_0" role="button" data-slide="Siguiente">
                    <span class="carousel-control-Siguiente-icon" aria-hidden="true"></span>
                    <span class="sr-only">Siguiente</span>
                </a>
            </div>
        </div>
    </div>
</div>
<hr />
